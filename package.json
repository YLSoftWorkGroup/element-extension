{
  "name": "element-extension",
  "version": "2.2.3",
  "description": "基于elementUI组件扩展组件库",
  "main": "lib/element-extension/element-extension.js",
  "scripts": {
    "pub": "npm run build && npm run build:docs",
    "build:docs": "webpages-packer  --env production --config packer-config.js --include index",
    "build": "webpages-packer  --env production --config packer-config.js --include elementExtension",
    "dev:docs": "webpages-packer --env development --config packer-config.js  --watch true --include index",
    "dev": "webpages-packer --env development --config packer-config.js  --watch true --include elementExtension",
    "start": "webpages-server --env development --config packer-config.js"
  },
  "repository": {
    "url": "https://github.com/yearrow/element-extension"
  },
  "homepage": "https://yearrow.github.io/element-extension/#/base",
  "keywords": [
    "vue",
    "component",
    "js",
    "ui"
  ],
  "author": "danielmlc",
  "license": "ISC",
  "devDependencies": {
    "dayjs": "^1.8.8",
    "highlight.js": "^9.3.0",
    "element-mini-ui": "^1.2.0",
    "vue": "^2.6.9",
    "vue-router": "^3.0.2",
    "vue-template-compiler": "^2.6.8",
    "webpages-packer": "1.1.17"
  },
  "dependencies": {
  }
}
