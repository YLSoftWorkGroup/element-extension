@import  './var.pcss';
.yl-flex-box {
  height: 100%;
  width: 100%;
  box-sizing: border-box;
  display: flex;
  & > .fixed-wrapper {
    display: block;
    & > div {
      width: 100%;
      height: 100%;
    }
  }
  & > .flex-wrapper {
    flex: 1;
    overflow:auto;
    & > div {
      width: 100%;
      height: 100%;
    }
  }
  & > .flod {
    display: block;
    background: $background-color-b;
    position: relative;
  }
}